# Quic-Fire Rough Terrain
# - Only runs where slope >= 20%
ModelExecution(?ex), 
hasModelInput(?ex, ?i),
hasLabel(?i, "topo.dat"),
hasDataBinding(?i, ?ds),
hasVariable(?ds, ?v),
hasLabel(?v, "mean_slope"),
hasValue(?v, ?val),
greaterThanOrEqual(?val, 20)
-> 
isValid(?ex, true),
hasValidityReason(?ex, "Mean slope >= 20%")

ModelExecution(?ex), 
hasModelInput(?ex, ?i),
hasLabel(?i, "topo.dat"),
hasDataBinding(?i, ?ds),
hasVariable(?ds, ?v),
hasLabel(?v, "mean_slope"),
hasValue(?v, ?val),
lessThan(?val, 20)
-> 
isValid(?ex, false),
hasValidityReason(?ex, "Mean slope should be >= 20%")
